#ifdef _MSC_VER
#define _CRT_SECURE_NO_WARNINGS
#endif
#include<stdio.h>
#include<stdbool.h>
#include<string.h>
#define N 40
#define MAX 15


int main()
{
	char students[6][30] = { "Rachel Green","Chandler Bing","Ross Geller",
	"Monica Geller","Phoebe Buffay","Joey Tribbiani" };

	int i;
	for (i = 0; i < 6; i++)
		printf("%s\n", students[i]);

	printf("\nStudents that start with R\n");
	for (i = 0; i < 6; i++)
		if (students[i][0] == 'R')
			printf("%s\n", students[i]);

	getch();
}
